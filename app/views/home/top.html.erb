
<div class="container">

<h5>対象者一覧</h5>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Section</th>
      <th>next</th>
      <th>Last</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seizou.each do |f| %>
      <tr>
        <td><%= f.name %></td>
        <td><%= f.section %></td>
        <td><%= f.next %></td>
        <td><%= f.last %></td>
      </tr>
    <% end %>
  </tbody>

  <tbody>
    <% @jimu.each do |f| %>
      <tr>
        <td><%= f.name %></td>
        <td><%= f.section %></td>
        <td><%= f.next %></td>
        <td><%= f.last %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
</div>